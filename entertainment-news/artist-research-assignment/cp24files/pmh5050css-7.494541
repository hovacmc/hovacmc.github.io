    #princessmargaretlotto{
  width:300px;
  height:170px;
  background-image:url(https://www.cp24.com/polopoly_fs/7.494621!/httpImage/image.png);
  position: relative;
  font-family: Arial, Helvetica, sans-serif;
}

#biggest{
  font-size: 13px;
  text-align: center;
  color: #FFF;
  position: absolute;
  top: 76px;
  width: 100%;
}

#fiftyfifty-container{
  top: 102px;
  left: 57px;
  text-align: center;
  position: absolute;
  width:236px;
}

#takeshalf{
  color:#000;
  font-size: 16px;
  font-weight: bold;
  position: absolute;
  top: 36px;
  width: 100%;
}

#licenses{
  font-size: 9px;
  position: absolute;
  top: 56px;
  width: 100%;
}

#fiftyfifty{
  background-color: #9A9A9A;
  box-shadow: 1px 2px 4px rgba(0, 0, 0, 0.75);
  border-radius: 4px;
  overflow: hidden;
  margin: 0 auto;
  height:31px;
}

/*dollar sign image*/
#fiftyfifty #fiftyfifty_prefix {
  font-size: 0;
  background-image:url(https://www.cp24.com/polopoly_fs/7.494626!/httpImage/image.png);
}
/*comma image*/
#fiftyfifty .counter_text{
  font-size: 0;
  background-image:url(https://www.cp24.com/polopoly_fs/7.494625!/httpImage/image.png);
}

.counter_character {
  display: inline;
  float: left;
  position: relative;
  margin-left: 1px;
  margin-right: 1px;
}

.counter_text {
  font-weight: bold;
  background-color: #000;
  font-size: 50px;
  line-height: 45px;
  text-align: center;
  color: #FFF;
}

.counter_character_left {
  margin-left: 0px;
  margin-right: 1px;
}

.counter_character_inner {
  margin-left: 1px;
  margin-right: 1px;
}

.counter_character_right {
  margin-left: 1px;
  margin-right: 0px;
}

.counter_marker {
  position: relative;
  z-index: 10;
}
